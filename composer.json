{
    "name": "amirreza/omnipay-azki-vam",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\AzkiVam\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\AzkiVam\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "amirreza.salari",
            "email": "amirrezasalari1997@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "omnipay/common": "^3",
        "symfony/http-client": "^5"
    },
    "require-dev": {
        "omnipay/tests": "^4",
        "php-http/guzzle7-adapter": "^1",
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3",
        "phpstan/phpstan": "^1.8",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
